{
  "name": "@kodingcaravan/shared",
  "version": "1.0.0",
  "description": "Shared utilities, types, and configurations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "require": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./config/*": {
      "require": "./dist/config/*.js",
      "types": "./dist/config/*.d.ts"
    },
    "./utils/*": {
      "require": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    },
    "./src/utils/*": {
      "require": "./dist/src/utils/*.js",
      "types": "./dist/src/utils/*.d.ts"
    },
    "./events/*": {
      "require": "./dist/events/*.js",
      "types": "./dist/events/*.d.ts"
    },
    "./databases/*": {
      "require": "./dist/databases/*.js",
      "types": "./dist/databases/*.d.ts"
    },
    "./databases/mongo/models": {
      "require": "./dist/databases/mongo/models/index.js",
      "types": "./dist/databases/mongo/models/index.d.ts"
    },
    "./middlewares/*": {
      "require": "./dist/middlewares/*.js",
      "types": "./dist/middlewares/*.d.ts"
    },
    "./types/*": {
      "require": "./dist/types/*.js",
      "types": "./dist/types/*.d.ts"
    },
    "./worker": {
      "require": "./dist/worker/index.js",
      "types": "./dist/worker/index.d.ts"
    },
    "./worker/*": {
      "require": "./dist/worker/*.js",
      "types": "./dist/worker/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.975.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "connect-timeout": "^1.9.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.1.4",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.0.3",
    "opossum": "^7.0.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/connect-timeout": "^0.0.36",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@types/opossum": "^8.1.9",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.8",
    "typescript": "^5.3.3"
  }
}
